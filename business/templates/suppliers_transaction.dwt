 //切换
  function toggle(obj, act, id)
  {
    var val = (obj.innerHTML.match(/是/i)) ? 0 : 1;
 
    var res = Ajax.call(this.url, "act="+act+"&val=" + val + "&id=" +id, null, "POST", "JSON", false);

    if (res.message)
    {
      alert(res.message);
    }
    if (res.error == 0)
    {
      obj.innerHTML = (res.content > 0) ? '是' : '否';
    }
  }

