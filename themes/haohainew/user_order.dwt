$(function () {
    H_login = {};
    H_login.openLogin = function(){
        $('.qfabu').click(function(){
            goods_id = $(this).data('id');
            order_id = $(this).data('order_id');
            luckdraw_id = $(this).data('luckdraw_id');
            $("#comment").val('');
            $('.common').show();
            $('.common-bg').show();
        });
    };
    H_login.closeLogin = function(){
        $('.close').click(function(){
            $('.common').hide();
            $('.common-bg').hide();
        });
    };
    H_login.run = function () {
        this.closeLogin();
        this.openLogin();
    };
    H_login.run();

    $("#btn-square").click(function(event) {
       var square = $.trim($("#square").val());
       console.info(square)
       if (square.length>0) {
        $.ajax({
            url: 'square_new.php?act=create',
            type: 'POST',
            dataType: 'json',
            data: {square: square,order_id: order_id,goods_id:goods_id,luckdraw_id: luckdraw_id},
            success: function (res) {
                if(res.isError == 1){
                    //alert(res.message)
					layer.open({
		                content: res.message,
			            btn: ['嗯']
		            });
                }
                else{
                    //alert('发布成功！')
					layer.open({
		                content: '发布成功！',
			            btn: ['嗯']
		            });
                    location.reload();
                }
            }
        })
        
       }
    });
});

